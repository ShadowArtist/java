//student has a adress with weak relationship(Association) known as Aggregation

class Address

{
String street_Num;
String city;
String state;
String country;


public Address (String s_num, String ci , String st, String co) 
{
	
street_Num=s_num;

city=ci;
state = st;

country=co;
	
	
}


}




